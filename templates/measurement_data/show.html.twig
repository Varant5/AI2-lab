{% extends 'base.html.twig' %}

{% block title %}MeasurementData{% endblock %}

{% block body %}
    <h1>MeasurementData</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ measurement_datum.id }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ measurement_datum.date ? measurement_datum.date|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Wind_speed</th>
                <td>{{ measurement_datum.windSpeed }}</td>
            </tr>
            <tr>
                <th>Humidity</th>
                <td>{{ measurement_datum.humidity }}</td>
            </tr>
            <tr>
                <th>Pressure</th>
                <td>{{ measurement_datum.pressure }}</td>
            </tr>
            <tr>
                <th>Temperature</th>
                <td>{{ measurement_datum.temperature }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_measurement_data_index') }}">back to list</a>

    <a href="{{ path('app_measurement_data_edit', {'id': measurement_datum.id}) }}">edit</a>

    {{ include('measurement_data/_delete_form.html.twig') }}
{% endblock %}
